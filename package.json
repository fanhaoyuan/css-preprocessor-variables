{
    "name": "sass-json-vars",
    "version": "1.0.0",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "repository": "https://github.com/fanhaoyuan/sass-json-vars.git",
    "author": "fansy <418536538@qq.com>",
    "license": "MIT",
    "description": "A plugin for convert sass vars to json.",
    "keywords": [
        "sass",
        "vars",
        "variables",
        "json"
    ],
    "scripts": {
        "build": "vite build && tsc"
    },
    "gitHooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "cmv"
    },
    "lint-staged": {
        "*.{json,md,ts,js}": [
            "prettier --write"
        ],
        "*.{ts,js}": [
            "eslint"
        ]
    },
    "dependencies": {
        "postcss": "^8.3.6",
        "postcss-sass": "^0.5.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@fansy/commit-msg-validator": "^1.0.0",
        "@fansy/eslint-config": "^1.0.0",
        "@fansy/prettier-config": "^1.0.0",
        "@jest/types": "^27.1.0",
        "@types/jest": "^27.0.1",
        "jest": "^27.1.0",
        "lint-staged": "^11.1.2",
        "ts-node": "^10.2.1",
        "vite": "^2.5.2"
    }
}
